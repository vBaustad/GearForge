    "npm-run-all2": "^8.0.4",
    "postcss": "^8.5.6",
    "prettier": "^3.8.1",
    "tailwindcss": "^4.1.18",
    "typescript": "~5.9.3",
    "typescript-eslint": "^8.56.0",
    "vitest": "^4.0.18"
